{
  "name": "emitry",
  "version": "1.2.1",
  "description": "Event emit and catch",
  "main": "./lib/emitry.es.js",
  "module": "./lib/emitry.es.js",
  "scripts": {
    "build-es": "rollup -c -f es -o ./lib/emitry.es.js",
    "build-es-min": "rollup -c -f es -o ./lib/emitry.es.min.js --environment min",
    "build-umd": "rollup -c -f umd -n Emitry -o ./lib/emitry.umd.js",
    "build-umd-min": "rollup -c -f umd -n Emitry -o ./lib/emitry.umd.min.js --environment min",
    "dev": "rollup -c -f umd -n Emitry -o ./lib/emitry.umd.js -w",
    "test": "jest",
    "add": "git add .",
    "release": "relix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PengJiyuan/emiton.git"
  },
  "keywords": [
    "event",
    "emit",
    "on",
    "off",
    "catch"
  ],
  "author": "PengJiyuan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PengJiyuan/emiton/issues"
  },
  "homepage": "https://github.com/PengJiyuan/emiton#readme",
  "devDependencies": {
    "jest": "^22.4.3",
    "pre-commit": "^1.2.2",
    "relix": "^1.0.0",
    "rollup": "^0.59.1",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-butternut": "^0.1.0"
  },
  "pre-commit": [
    "build-es",
    "build-es-min",
    "build-umd",
    "build-umd-min",
    "add",
    "test"
  ]
}
